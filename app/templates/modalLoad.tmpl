{{define "modalLoad"}}
<!-- Modal Load Sessions in Google Sheets -->
<div class="modal fade" id="modalLoad" tabindex="-1" role="dialog" aria-labelledby="modalLoadTitle" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalLoadTitle">Load</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" id="modalLoadBody">
        <ul id="sheetList" class="modal-ul">
        </ul>
      </div>
      <div class="modal-footer">
        <button type="button" id="loadModalBtn" class="btn btn-primary">Load</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
{{ end }}
